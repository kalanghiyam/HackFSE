<workbook>
  <worksheet idx="0">
    <section startRow="0" endRow="0" />
    <loop startRow="1" endRow="1" items="result" var="empInfo" varType="com.cts.fse.feedback.bean.EventEmployeeInfo">
      <section startRow="1" endRow="1">
       	<mapping row="1" col="0">empInfo.eventEmployeeIdentity.eventId</mapping>
        <mapping row="1" col="2">empInfo.beneficiaryName</mapping>
        <mapping row="1" col="3">empInfo.baseLocation</mapping>
        <mapping row="1" col="4">empInfo.eventDateString</mapping> 
        <mapping row="1" col="5">empInfo.eventEmployeeIdentity.associateId</mapping>
      </section>
      <loopbreakcondition>
        <rowcheck offset="0">
          <cellcheck offset="0" />
        </rowcheck>
      </loopbreakcondition>
    </loop>
  </worksheet>
</workbook>